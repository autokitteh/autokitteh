{
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "moduleResolution": "node",
    "baseUrl": ".",
    "rootDir": "src",
    "resolveJsonModule": true,
    "declaration": true,
    "sourceMap": true,
    "outDir": "dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "noImplicitReturns": false,
    "paths": {
      "autokitteh/*": [
        "../../nodejs/nodejs-sdk/autokitteh/*"
      ]
    }
  },
  "include": [
    "src/**/*.ts"
  ]
}
