# Preconditions: create project and connection.
ak project create my_project
return code == 0
output equals project_id: prj_00000000000000000000000001

ak connection create my_connection --project my_project --integration http --connection-token my_url_path
return code == 0
output equals 'connection_id: con_00000000000000000000000002'

# Negative tests: get nonexistent event, with/out --fail flag.
ak event get evt_00000000000000000000000003
return code == 0
output equals ''

ak event get evt_00000000000000000000000003 --fail
output equals 'Error: event not found'
return code == 10

# Send HTTP GET request to create new event.
http get /http/my_url_path
resp code == 200

# Get first event, with/out JSON.
ak event get evt_00000000000000000000000003
output contains ''
