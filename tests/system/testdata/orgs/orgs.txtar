user zumi

ak orgs create --display-name meow
return code == 0
capture_jq meow_oid .org_id

ak orgs get $meow_oid -f
return code == 0
output equals_jq .org.display_name meow

ak orgs list-members $meow_oid
return code == 0
output equals_jq . [$ZUMI_UID]

ak orgs update $meow_oid --display-name hiss
return code == 0

ak orgs get $meow_oid -f
return code == 0
output equals_jq .org.display_name hiss

ak users get-orgs
return code == 0
output equals_jq .orgs[].org_id $meow_oid,$CATS_OID,$ZUMI_OID

ak orgs delete $meow_oid
return code == 0

ak orgs get $meow_oid -f
return code == $RC_NOT_FOUND

ak users get-orgs
return code == 0
output equals_jq .orgs[].org_id $CATS_OID,$ZUMI_OID


-- test-config.yaml --
ak:
    extra_args: ["-j", "--array_json_list"]
