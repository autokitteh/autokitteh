-- project.yaml --
version: v1

project:
  name: myproject
  connections:
    - name: myhttp
      integration: http
  triggers:
    - name: http_start
      connection: myhttp
      event_type: get
      data:
        path: start
      call: program.star:on_http_start

-- program.star --
def on_http_start():
  s = subscribe("myhttp", "data.url.path == '/meow'")
  print(next_event(s)['body'].text())
  print(next_event(s)['body'].text())
