-- project.yaml --
version: v1

project:
  name: myproject
  triggers:
    - name: http_start
      type: webhook
      call: program.py:on_http_start
    - name: http1
      type: webhook
    - name: http2
      type: webhook

-- program.py --
from autokitteh import subscribe, next_event

def on_http_start(_):
  s1 = subscribe("http2", "true")
  s2 = subscribe("http1", "true")
  print("ready")
  print(1, next_event([s2])['body']['bytes'])
  print(2, next_event([s1])['body']['bytes'])
  print(3, next_event([s2], timeout=1))
  print(4, next_event([s1, s2], timeout=2)['body']['bytes'])
  print(5, next_event([s1, s2], timeout=2)['body']['bytes'])
  print("done")
