from autokitteh import mutate_value, get_value, set_value, list_values_keys, del_value, add_values, check_and_set_value

def main(_):
    print(mutate_value('cat', 'del'))
    print(mutate_value('dog', 'del'))
    print(mutate_value('dog', 'get'))
    print(set_value('cat', 'meow'))
    print(get_value('cat'))
    print(list_values_keys())

    print(set_value('dog', 'woof'))
    print(get_value('dog'))
    print(list_values_keys())

    print(add_values('x', 1))
    print(get_value('x'))
    print(add_values('x', 41))
    print(get_value('x'))
    print(check_and_set_value('x', 42, 100))
    print(get_value('x'))
    print(check_and_set_value('x', 42, 101))
    print(get_value('x'))
