from autokitteh import AutoKittehError, activity, next_event


@activity
def problem():
    try:
        next_event('event')
    except AutoKittehError as err:
        text = str(err)
    # We don't compare the exact message since it's brittle
    print('next_event' in text)
    print('not allowed' in text)
    print('activity' in text)


def main(event):
    print('main start')
    problem()
    print('main end')
