import autokitteh

# Will cause recursion error when unpickled
class Infi:
    def __init__(self):
        self._lookup = {}

    def __getattr__(self, attr):
        if attr in self._lookup:
            return self._lookup[attr]
        raise AttributeError(attr)


def main(event):
	get_infi()


@autokitteh.activity
def get_infi():
	return Infi()
