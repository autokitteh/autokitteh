from autokitteh import activity

# This exception can be pickled, but not unpickled.
class X(Exception):
    def __init__(self, a, b):
        self._a, self._b = a, b
        super(X, self).__init__(a, b)


@activity
def callstuff():
    raise X(1, 2)


def main(_):
    callstuff()

